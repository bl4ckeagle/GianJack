{% extends 'AdminTemplate1.html.twig ' %}



{% block rightside %}







    <table class="table table-striped table-hover">
        <caption> <h1>Videos</h1> </caption>
        <thead>
        <tr>
            <th>#</th>
            <th>Title</th>

            <th>Date</th>
            <th>Edit</th>
            <th>Delete</th>

        </tr>
        </thead>
        <tbody>
        {% for album in Album  %}
            <tr>
                <td>{{ album.albenId}}</td>
                <td>{{ album.name}}</td>
                <td>{{ album.releasedate|date("m.d.Y")}}</td>
                <td>
                    <button class="btn btn-warning" onclick="editing({{ album.albenId}})">Edit</button>
                </td>
                <td>
                    <button class="btn  btn-danger " onclick="deleting({{ album.albenId }}) ">Delete</button>
                </td>
            </tr>


        {% endfor %}

        </tbody>


    </table>

    <div class="container-fluid">
        <td><button class="btn btn-primary" onclick="inserting()">Create New</button></td>


    </div>





{% endblock %}
