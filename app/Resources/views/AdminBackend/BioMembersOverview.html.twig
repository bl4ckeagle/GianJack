{% extends 'AdminTemplate1.html.twig ' %}

{% block rightside %}

    <table class="table table-striped table-hover">
<caption> <h1>Members</h1> </caption>
<thead>
<tr>

    <th>First Name</th>
    <th>Last Name</th>
    <th>Edit</th>
    <th>Delete</th>

</tr>
</thead>
<tbody>
{% for members in members %}
    <tr>
        <td>{{ members.firstName }}</td>
        <td>{{ members.lastName }}</td>

        <td>
            <button class="btn btn-warning" onclick="editing({{ members.biographyMemberId }})">Edit</button>
        </td>
        <td>
            <button class="btn  btn-danger " onclick="deleting({{ members.biographyMemberId }}) ">Delete</button>
        </td>
    </tr>


{% endfor %}

</tbody>


</table>

<div class="container-fluid">
<td><button class="btn btn-primary" onclick="inserting()">Create New</button></td>
</div>
{{ dump() }}

{% endblock %}