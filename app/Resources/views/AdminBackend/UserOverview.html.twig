{% extends 'AdminTemplate1.html.twig ' %}



{% block rightside %}







    <table class="table table-striped table-hover">
        <caption> <h1>User</h1> </caption>
        <thead>
        <tr>
            <th>Name</th>
            <th>LastName</th>
            <th>Username</th>
            <th>Edit</th>


        </tr>
        </thead>
        <tbody>
        {% for users in User  %}
            <tr>
                <td>{{ users.firstName}}</td>
                <td>{{ users.lastName}}</td>
                <td>{{ users.username}}</td>
                <td>
                    <button class="btn btn-warning" onclick="editing({{ users.username}})">Edit</button>
                </td>

            </tr>


        {% endfor %}

        </tbody>


    </table>

    <div class="container-fluid">
        <td><button class="btn btn-primary" onclick="inserting()">Create New</button></td>


    </div>




    {{ dump() }}
{% endblock %}
