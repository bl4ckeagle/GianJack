<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8"/>
    {% block stylesheets %}
        <link href="{{ asset('bundles/framework/css/main.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/framework/css/bootstrap.min.css') }}" rel="stylesheet"/>

    {% endblock %}

    <title>{% block title %}Admin Template Change{% endblock %}</title>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

</head>

{% block masterbody %}
<body>

<div class="container-fluid">

    <div class="row">
        <div class="col-md-1">
            {% block leftside %}

                <nav class="navbar navbar-default" role="navigation">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse"
                                data-target=".navbar-ex1-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">AdminArea</a>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse navbar-ex1-collapse">
                        <ul class="nav navbar-nav">

                            <li><a href={{ path('Backendhome') }}>Overview</a></li>
                            <li><a href="{{ path('VideoBackEnd') }}">Videos</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="{{ path('logout') }}">Logout </a> </li>

                        </ul>
                    </div><!-- /.navbar-collapse -->
                </nav>


            {% endblock %}


        </div>

        <div class="col-md-11">

            {% block rightside %}

            {% endblock %}
                </div>


        </div>


    </div>


</div>


{% block javascripts %}
    <script src="{{ asset('bundles/framework/js/jquery-2.2.3.min.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/bootstrap.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/myscripts/ButtonHandler.js') }}"> </script>


{% endblock %}

</body>
</html>
{% endblock %}