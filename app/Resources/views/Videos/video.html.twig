{% extends 'BaseTemplate1.html.twig ' %}


{% block title %}Videos{% endblock %}
{% block contentmaster %}
    <div class="videos">

        <div class="jumbotron">

            {{ dump() }}

        </div>


        <div>
            <div class="container-fluid">
                <div class="row">
                    {% for alb in album %}
                        <div class="col-md-6">
                            <div class="jumbotron">

                                <a href="{{ path('videos') }}/{{ alb.albenID }}">{{ alb.name }}</a>
                            </div>
                        </div>
                    {% endfor %}
                    <div class="row">
                    </div>

                </div>
            </div>
        </div>
    </div>


{% endblock %}
